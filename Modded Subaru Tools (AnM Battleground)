local player = game.Players.LocalPlayer

local Greg = Instance.new("Folder")
Greg.Name = "Greg"
Greg.Parent = workspace

local first = player.Backpack:FindFirstChild("Shamac")
if first then
	first.Parent = Greg
end

local second = player.Backpack:FindFirstChild("Rental Goa")
if second then
	second.Parent = Greg
end

local third = player.Backpack:FindFirstChild("REMM !")
if third then
	third.Parent = Greg
end

local fourth = player.Backpack:FindFirstChild('"I can return from death"')
if fourth then
	fourth.Parent = Greg
end

local fifth = player.Backpack:FindFirstChild("Reinhard !!")
if fifth then
	fifth.Parent = Greg
end

local sixth = player.Backpack:FindFirstChild("Return By Death")
if sixth then
	sixth.Parent = Greg
end

local tool1 = Instance.new("Tool")
tool1.Name = "Modded Shamac"
tool1.RequiresHandle = false
tool1.CanBeDropped = false
tool1.Parent = player.Backpack

tool1.Activated:Connect(function()
	for i = 1, 1 do --Configuration change the 1 before do to spam
		Greg:FindFirstChild("Shamac").LocalScript.Event:FireServer()
	end
end)

